{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "build": {
      "env": {
        "PKG_CONFIG_PATH": "/usr/lib/pkgconfig:/usr/share/pkgconfig",
        "PKG_CONFIG_LIBDIR": "/usr/lib/pkgconfig",
        "NODE_VERSION": "18" 
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  